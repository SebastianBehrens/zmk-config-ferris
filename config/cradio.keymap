    macros {
        sql_comment: sql_comment {
            label = "sql_comment";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp K>
                , <&macro_tap &kp C>
                , <&macro_release &kp LCTRL>
                , <&macro_tap &kp RIGHT>
                ;
        };
        sql_uncomment: sql_uncomment {
            label = "sql_uncomment";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp K>
                , <&macro_tap &kp U>
                , <&macro_release &kp LCTRL>
                , <&macro_tap &kp RIGHT>
                ;
        };
        numpadcomma: numpadcomma {
            label = "numpadcomma";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp COMMA>
                , <&macro_tap &kp SPACE>
                ;
        };
        CAPITALAE: CAPITALAE {
            label = "CAPITALAE";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp CAPS>
                , <&macro_tap &kp SQT>
                , <&macro_tap &kp CAPS>
                ;
        };
        CAPITALOE: CAPITALOE {
            label = "CAPITALOE";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp CAPS>
                , <&macro_tap &kp SEMI>
                , <&macro_tap &kp CAPS>
                ;
        };
        CAPITALUE: CAPITALUE {
            label = "CAPITALUE";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp CAPS>
                , <&macro_tap &kp LBKT>
                , <&macro_tap &kp CAPS>
                ;
        };
        MYCARET: MYCARET {
            label = "MYCARET";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp EQUAL>
                , <&macro_tap &kp SPACE>
                ;
        };
        MYTILDEMAC: MYTILDEMAC {
            label = "MYTILDEMAC";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LALT>
                , <&macro_tap &kp N>
                , <&macro_release &kp LALT>
                , <&macro_tap &kp SPACE>
                ;
        };
        MYTILDEWIN: MYTILDEWIN {
            label = "MYTILDEWIN";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RALT>
                , <&macro_tap &kp EQUAL>
                , <&macro_release &kp RALT>
                , <&macro_tap &kp SPACE>
                ;
        };
        MYCODEDELIM:  MYCODEDELIM {
            label = "MYCODEDELIM";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_tap &kp RS(EQUAL)>
                , <&macro_tap &kp SPACE>
                ;
        };
