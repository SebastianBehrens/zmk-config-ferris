//          &none  &none      &none      &none     &none           &none   &bt_macro_win &bt BT_SEL 5 &none     &none
//        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
//        //│          │          │          |          |          │   │          │Profile 1 │Profile 2 │Profile 3 │          │
//            &none      &none      &none     &none        &none         &none     &bt_macro_mac &bt_macro_ipad &bt_macro_iphone &none 
//        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
//                                             &none      &none          &none      &none 
//        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
//            >;
//        };

// LAYER 4
        bluetooth_layer1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│          │          │          │          |          │   │ CLEAR BT │          │          │          │          │
            &none     &none      &none      &none     &none          &bt BT_CLR &none      &none      &none       &sys_reset
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          │          |          │   │          │Profile 4 │Profile 5 │          │          │
          &none  &none      &none      &none     &none           &none   &bt BT_SEL 4 &bt BT_SEL 5 &none     &none
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          |          |          │   │          │Profile 1 │Profile 2 │Profile 3 │          │
            &none      &none      &none     &none        &none         &none     &bt BT_SEL 1 &bt BT_SEL 2 &bt BT_SEL 3 &none 
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                             &none      &none          &to 0      &to 5 
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

 // LAYER 5
        default_layer2 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│  Q       │  W       │  E       │  R       │  T       │   │  Z       │  U       │  I       │  O       │  P       │
            &kp Q      &kp W      &kp E     &lh_pht LCTRL R &kp T          &kp Y      &rh_pht LCTRL U      &kp I      &kp O      &kp P
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  A       │  S       │  D       │  F       │  G       │   │  H       │  J       │  K       │  L       │  "      │
            &kp A      &lt 9 S    &kp D       &kp F    &ht LA(TAB) G   &kp H      &kp J       &kp K     &kp L      &kp LS(N2)
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  Y       │  X       │  C       │  V       │  B       │   │  N       │  M       │ , <      │ . >      │ / ?      │
           &lh_pht LSHIFT Z &kp X &lh_pht LALT  C &lh_pht LGUI V    &kp B          &kp N     &rh_pht LGUI M  &rh_pht RALT COMMA &kp DOT &rh_pht LSHIFT FSLH
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                            
                                            &lqt_td2 8 0 &cht 11 LSHIFT     &kp SPACE  &lqt 7 ESCAPE
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

// LAYER 6
        work {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│  Q       │  W       │  E       │  R       │  T       │   │  Z       │  U       │  I       │  O       │  P       │
            &kp Q      &kp W      &kp E     &lh_pht LCTRL R &kp T          &kp Y      &rh_pht LCTRL U      &kp I      &kp O      &kp P
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  A       │  S       │  D       │  F       │  G       │   │  H       │  J       │  K       │  L       │  "      │
            &kp A      &lt 9 S    &kp D     &kp F    &ht LA(TAB) G     &kp H      &kp J      &kp K      &kp L     &kp LS(N2)
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  Y       │  X       │  C       │  V       │  B       │   │  N       │  M       │ , <      │ . >      │ / ?      │
           &lh_pht LSHIFT Z &kp X    &kp C       &kp V       &kp B          &kp N     &kp  M  &kp COMMA &kp DOT &rh_pht LSHIFT FSLH
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                            &lqt_td2 8 0 &kp LSHIFT     &kp SPACE  &lqt 7 ESCAPE
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

// LAYER 7
        symbol_layer2 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
