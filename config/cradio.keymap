#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

&sl {
    release-after-ms = <500>;
};

/ {

    behaviors {
        ht: hold_tap {
            label = "hold_tap";
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <150>;
            quick-tap-ms = <70>;
            global-quick-tap;
            bindings = <&kp>, <&kp>;
        };
        lh_pht: left_hand_positional_hold_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "LEFT_POSITIONAL_HOLD_TAP";
            #binding-cells = <2>;
            flavor = "tap-unless-interrupted";
            tapping-term-ms = <100>;                        // <---[[produces tap if held longer than tapping-term-ms]]
            quick-tap-ms = <200>;
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <6 7 8 9 10 16 17 18 19 20 26 27 28 29 30 33 34>;    // <---[[right-hand keys]]
        };
        rh_pht: left_hand_positional_hold_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "LEFT_POSITIONAL_HOLD_TAP";
            #binding-cells = <2>;
            flavor = "tap-unless-interrupted";
            tapping-term-ms = <100>;                        // <---[[produces tap if held longer than tapping-term-ms]]
            quick-tap-ms = <200>;
            bindings = <&kp>, <&kp>;
            hold-trigger-key-positions = <1 2 3 4 5 11 12 13 14 15 21 22 23 24 25 31 32 >;    // <---[[left-hand keys]]
        };
        cht: custom_hold_tap {
            compatible = "zmk,behavior-hold-tap";
            label = "nested_ht_with_sk";
            #binding-cells = <2>;
            tapping-term-ms = <500>;
            flavor = "tap-preferred";
            bindings = <&mo>, <&skq>;
        };
        ht_reset: hold_tap_reset {
            label = "hold_tap_reset";
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "tap-preferred";
            tapping-term-ms = <3000>;
            quick-tap-ms = <150>;
            bindings = <&bootloader>, <&none>;
        };
      skq: sticky_key_quick_release {
           compatible = "zmk,behavior-sticky-key";
           label = "STICKY_KEY_QUICK_RELEASE";
           #binding-cells = <1>;
           bindings = <&kp>;
           release-after-ms = <700>;
           quick-release;
            };
            // global quick tap to improve home row mod behavior ior when typing fast
      gqt: global-quick-tap { 
           compatible = "zmk,behavior-hold-tap";
           label = "GLOBAL_QUICK_TAP";
           #binding-cells = <2>;
           flavor = "tap-preferred";
           tapping-term-ms = <200>;
           quick-tap-ms = <125>;
           global-quick-tap;
           bindings = <&kp>, <&kp>;
        };
        //custom hold tap for layer tap - lqt
      lqt: layer-quick-tap { 
           compatible = "zmk,behavior-hold-tap";
           label = "LAYER_QUICK_TAP";
           #binding-cells = <2>;
           flavor = "hold-preferred";
           tapping-term-ms = <200>;
           quick-tap-ms = <125>;
           bindings = <&mo>, <&kp>;
        };
      lqt_td: layer-quick-tap-tap-dance { 
           compatible = "zmk,behavior-hold-tap";
           label = "LAYER_QUICK_TAP_TAP_DANCE";
           #binding-cells = <2>;
           flavor = "hold-preferred";
           tapping-term-ms = <200>;
           quick-tap-ms = <225>;
           bindings = <&mo>, <&td1>;
        };
      lqt_td2: layer-quick-tap-tap-dance2 { 
           compatible = "zmk,behavior-hold-tap";
           label = "LAYER_QUICK_TAP_TAP_DANCE2";
           #binding-cells = <2>;
           flavor = "hold-preferred";
           tapping-term-ms = <200>;
           quick-tap-ms = <225>;
           bindings = <&mo>, <&td2>;
        };
      td1: td1 {
           compatible = "zmk,behavior-tap-dance";
           label = "td_left_thumb_1";
           #binding-cells = <0>;
           tapping-term-ms = <200>;
           bindings =
           <&kp BSPC>, <&kp LA(BSPC)>;
          };
      td2: td2 {
           compatible = "zmk,behavior-tap-dance";
           label = "td_left_thumb_2";
           #binding-cells = <0>;
           tapping-term-ms = <200>;
           bindings =
           <&kp BSPC>, <&kp LC(BSPC)>;
          };
    };

