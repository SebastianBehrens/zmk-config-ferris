// layernames will have the ending digit 1 if they are intended for use with MACOS
// layernames will have the ending digit 2 if they are intended for use with WINDOWS

// LAYER 0
    keymap {
        compatible = "zmk,keymap";
        default_layer1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│  Q       │  W       │  E       │  R       │  T       │   │  Z       │  U       │  I       │  O       │  P       │
            &kp Q      &kp W      &kp E     &lh_pht LCTRL R &kp T          &kp Y    &rh_pht RCTRL U   &kp I      &kp O      &kp P
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  A       │  S       │  D       │  F       │  G       │   │  H       │  J       │  K       │  L       │  "      │
            &kp A      &lt 3 S    &kp D      &kp F      &ht LG(TAB) G   &kp H     &kp J       &kp K     &kp L     &kp LS(N2)
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│  Y       │  X       │  C       │  V       │  B       │   │  N       │  M       │ , <      │ . >      │ / ?      │
           &lh_pht LSHIFT Z &kp X &lh_pht LALT  C &lh_pht LGUI V    &kp B          &kp N     &rh_pht LGUI M  &rh_pht RALT COMMA &kp DOT &rh_pht LSHIFT FSLH
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                            
                                            &lqt_td 2 0 &cht 16 LSHIFT     &ht LC(LA(LSHIFT)) SPACE  &lqt 1 ESCAPE
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

// LAYER 1
        symbol_layer1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│ +        │  "       │ \#       │  %       │    &     │   │   \`     │   /      │   \      │   =      │ caret    |
            &kp LS(N1) &kp LS(N2) &kp LA(N3) &kp LS(N5)  &kp LS(N6)    &MYCODEDELIM  &kp LS(N7)  &kp LS(LA(N7)) &kp LS(N0) &MYCARET 
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │  {       │  [       │  (       │          │   │          │    )     │  ]       │     }    │  '       │
           &none   &kp LA(N8) &kp LA(N5) &kp LS(N8) &kp LA(G)        &none  &kp LS(N9) &kp LA(N6) &kp LA(N9) &kp MINUS
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│ <        │  >       │  *       │  9       │          │   │ tilde    │  pipe    │  ?       │     !    │  $       │     
           &kp GRAVE   &kp LS(GRAVE) &kp LS(N3) &none    &none      &MYTILDEMAC    &kp LA(N7)   &kp LS(MINUS) &kp LS(RBKT) &ht LSHIFT BSLH
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                             &lt 14 ESCAPE &kp TAB   &ht_reset 0 0  &none 
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

// LAYER 2
        number_layer1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│          │          │          │          │          │   │          │  7       │  8       │  9       │         │
            &mo 13      &CAPITALAE     &CAPITALOE      &CAPITALUE      &none          &none      &kp N7     &kp N8     &kp N9   &none 
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │ ä        │   ö      │   ü      │          │   │     -    │  4       │  5       │  6       │          │
           &caps_word      &kp SQT     &kp SEMI     &kp LBKT  &none    &kp FSLH &kp N4     &kp N5     &kp N6     &kp DOT
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          │          │          │   │     /    │  1       │   2      │    3     │          │
            &none      &none      &none      &none       &none         &kp LS(N7)     &kp N1    &kp N2     &kp N3     &numpadcomma
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                             &none  &ht_reset 0 0  &kp ENTER   &kp N0
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

// LAYER 3
        vim_layer1 {
            bindings = <
        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
        //│          │          │          │          │          │   │          │          │          │          │         │
            &none      &none     &none      &none      &mo 4           &kp RC(RS(TAB))      &kp PG_UP     &none   &kp RC(TAB)   &none 
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          │          │          │   │          │          │          │          │          │
           &none      &none      &kp PG_DN     &none       &none         &kp LEFT    &kp DOWN   &kp UP    &kp RIGHT  &none 
        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
        //│          │          │          │          │          │   │          │          │          │          │          │
            &kp LSHIFT      &none      &to 14      &none      &none          &kp LG(LEFT)      &kp LG(HOME)    &kp LG(END)     &kp LG(RIGHT)     &none
        //╰──────────┴──────────┴──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┴──────────┴──────────╯
                                             &none      &none          &kp ENTER      &kp LG(ENTER)
        //                                 ╰──────────┴──────────╯   ╰──────────┴──────────╯
            >;
        };

//// LAYER 4
//        bluetooth_layer1 {
//            bindings = <
//        //╭──────────┬──────────┬──────────┬──────────┬──────────╮   ╭──────────┬──────────┬──────────┬──────────┬──────────╮
//        //│          │          │          │          |          │   │ CLEAR BT │          │          │          │          │
//            &none     &none      &none      &none     &none          &bt BT_CLR &none      &none      &none       &sys_reset
//        //├──────────┼──────────┼──────────┼──────────┼──────────┤   ├──────────┼──────────┼──────────┼──────────┼──────────┤
